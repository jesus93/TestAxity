@model WebSite.Models.LoginModel
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<div class="row">
    <div class="col-md-4">
        Nombre
    </div>
    <div class="col-md-4">
        Password
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        @Html.TextBoxFor(model => model.Nombre, new { @id = "nameText" })
    </div>
    <div class="col-md-4">
        @Html.TextBoxFor(model => model.Password, new { @id = "passText" })
    </div>
</div>
<div class="row">
    <div class="col-md-8">

    </div>
    <div class="col-md-4">
        <button type="submit" id="LoginBtn" class="btn btn-primary">Iniciar sesión</button>
    </div>
</div>